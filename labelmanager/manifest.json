{
  "name": "Livestream Alerts - Label Manager",
  "version": "0.3",
  "manifest_version": 2,
  "minimum_chrome_version": "31",
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    {"fileSystem": ["write", "retainEntries", "directory"]},
    "storage", "https://www.livestreamalerts.com/*"
  ]
}
